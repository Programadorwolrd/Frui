generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model bebida {
  id         Int     @id @default(autoincrement())
  nome       String
  preco      String
  quantidade String?
}

model comida {
  id    Int    @id @default(autoincrement())
  nome  String
  preco String
}
